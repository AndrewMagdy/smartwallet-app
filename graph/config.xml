<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jolocom.graph" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Graph</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="CodePushDeploymentKey" value="YaLoyv3FaE8Yoll11ieegqf2fk_YVkWBGkm5e" />
        <icon src="res/android/ldpi/icon.png" density="ldpi" />
        <icon src="res/android/mdpi/icon.png" density="mdpi" />
        <icon src="res/android/hdpi/icon.png" density="hdpi" />
        <icon src="res/android/xhdpi/icon.png" density="xhdpi" />
        <icon src="res/android/xxhdpi/icon.png" density="xxhdpi" />
        <icon src="res/android/xxxhdpi/icon.png" density="xxxhdpi" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="CodePushDeploymentKey" value="PtVM-Mez6bYbitXdE88YeYZvUNfzVkWBGkm5e" />
        <!-- iOS 8.0+ -->
        <!-- iPhone 6 Plus  -->
        <icon src="res/ios/app-icon@180x180.png" width="180" height="180" />
        <!-- iOS 7.0+ -->
        <!-- iPhone / iPod Touch  -->
        <icon src="res/ios/app-icon@60x60.png" width="60" height="60" />
        <icon src="res/ios/app-icon@120x120.png" width="120" height="120" />
        <!-- iPad -->
        <icon src="res/ios/app-icon@76x76.png" width="76" height="76" />
        <icon src="res/ios/app-icon@152x152.png" width="152" height="152" />
        <!-- Spotlight Icon -->
        <icon src="res/ios/app-icon@80x80.png" width="40" height="40" />
        <icon src="res/ios/app-icon@80x80.png" width="80" height="80" />
        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="res/ios/app-icon@29x29.png" width="29" height="29" />
        <icon src="res/ios/app-icon@58x58.png" width="58" height="58" />
        <!-- iPad Spotlight and Settings Icon -->
        <icon src="res/ios/app-icon@50x50.png" width="50" height="50" />
        <icon src="res/ios/app-icon@120x120.png" width="100" height="100" />
        <!-- iPad Pro -->
        <icon src="res/ios/app-icon@167x167.png" width="167" height="167" />
    </platform>
</widget>
